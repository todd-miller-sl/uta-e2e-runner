{
  "name": "@united-talent-agency/touring-calculations",
  "version": "1.0.2",
  "description": "Common calculations used in the touring application.",
  "main": "lib/index.js",
  "author": "united-talent-agency",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
